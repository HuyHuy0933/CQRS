{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionString": "Server=.;Database=SalaryDb;Integrated Security=True;MultipleActiveResultSets=true",
  "Cors": {
    "AllowedOrigins": [ "*" ]
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "Host": "rabbitmq://host.docker.internal:5672",
    "Username": "guest",
    "Password": "guest"
  },
  "Auth": {
    "Authority": "http://host.docker.internal:5401"
  },
  "DataProtectionConfiguration": {
    "KeyPath": "C:\\keys"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
